{
  "name": "mgnrega-goa-dashboard-monorepo",
  "version": "1.0.0",
  "description": "MGNREGA Goa Dashboard - Monorepo containing backend and frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "node test-setup.js",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "deploy:check": "node test-setup.js"
  },
  "keywords": [
    "mgnrega",
    "goa",
    "dashboard",
    "railway",
    "deployment"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "axios": "^1.12.2"
  }
}
